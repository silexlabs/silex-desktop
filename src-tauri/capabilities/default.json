{
  "identifier": "default",
  "description": "Default capabilities for Silex desktop",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:event:default",
    "core:event:allow-emit",
    "core:window:default",
    "core:window:allow-maximize",
    "core:window:allow-unmaximize",
    "core:window:allow-set-size",
    "core:window:allow-center",
    "dialog:default"
  ]
}
