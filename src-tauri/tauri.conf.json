{
  "$schema": "https://raw.githubusercontent.com/tauri-apps/tauri/dev/crates/tauri-config-schema/schema.json",
  "productName": "Silex",
  "version": "0.1.0",
  "identifier": "org.silex.desktop",
  "build": {
    "frontendDist": "./frontend"
  },
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "title": "Silex",
        "width": 1280,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.png"
    ]
  }
}
